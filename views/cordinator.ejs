<%- include('header'); -%>

<!-- ======= Team Section ======= -->
<section id="team" class="team">
    <div class="container" data-aos="fade-up">

      <div class="section-header">
        <h2>Our Coordinators</h2>
        <p>"The backbone of the classroom: dedicated coordinators ensuring the success of every student.From schedules to support, our coordinators keep the class on track and thriving."</p>
      </div>




      <div class="row gy-5">
        <% data.forEach((document) => { %>
          <div class="col-xl-4 col-md-6 d-flex" data-aos="zoom-in" data-aos-delay="200">
            <div class="team-member">
              <div class="member-img">
                <img src="assets/img/team/team-1.jpg" class="img-fluid" alt="">
              </div>
              <div class="member-info">
                <div class="social">
                  <a href=""><i class="bi bi-twitter"></i></a>
                  <a href=""><i class="bi bi-facebook"></i></a>
                  <a href=""><i class="bi bi-instagram"></i></a>
                  <a href=""><i class="bi bi-linkedin"></i></a>
                </div>
                <h4><%= document.name %></h4>
                <span> Class:- <%= document.class %></span>
                <span>Contact:-  <%= document.Contact %></span>
              </div>
            </div>
          </div><!-- End Team Member -->
        <% }) %>
      </div>
      





      

        
        
      </div>

    </div>
  </section><!-- End Team Section -->




<%- include('footer'); -%>