<%- include('header'); -%>
<!-- ======= Team Section ======= -->


      <section id="team" class="team">
        <div class="container" data-aos="fade-up">
          <div class="section-header">
            <h2>Student List of <%= className %></h2>
            <p>"Preparing today's students for tomorrow's challenges."</p>
          </div>
          <div class="row gy-5">
            <% data.forEach((document) => {
              if (document.class ===   className ) { %>
                <div class="col-xl-4 col-md-6 d-flex" data-aos="zoom-in" data-aos-delay="200">
                  <div class="team-member">
                    <div class="member-img">
                      <img src="assets/img/team/team-1.jpg" class="img-fluid" alt="">
                    </div>
                    <div class="member-info">
                      <h4><%= document.name %></h4>
                      <span> Class:- <%= document.class %></span>
                      <span>place:-  <%= document.place %></span>
                      <span>Contact:-  <%= document.contact %></span>
                    </div>
                  </div>
                </div><!-- End Team Member -->
              <% }
            }); %>
          </div>
       
      
      



      

        
        
      </div>

    </div>
  </section><!-- End Team Section -->




